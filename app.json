{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "react-native-health",
      "expo-secure-store"
    ],
    "name": "goodwager",
    "slug": "goodwager",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d967a9ec-1ed9-4784-82ed-b22775c100aa"
      }
    },
    "ios": {
      "infoPlist": {
        "NSHealthShareUsageDescription": "This app requires access to your health data to track your workouts and heart rate.",
        "NSHealthUpdateUsageDescription": "This app does not write health data."
      },
      "entitlements": {
        "com.apple.developer.healthkit": true
      },
      "bundleIdentifier": "com.harleysiezar.goodwager",
      "buildNumber": "10"
    },
    "icon": "./app/assets/images/icon.png",
    "orientation": "portrait"
  }
}
